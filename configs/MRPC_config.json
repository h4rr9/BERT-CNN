{
  "exp":{
    "name": "conv_mrpc"
  },

  "data_loader":{
    "name": "MRPCDataLoader",
    "batch_size": 128
  },

  "model":{
    "name": "MRPCModel",
    "optimizer": "adam"
  },

  "trainer":{
    "name": "MRPCModelTrainer",
    "num_epochs": 10,
    "verbose_training": true
  },

  "callbacks":{
    "checkpoint_monitor": "val_f1_score",
    "checkpoint_mode": "max",
    "checkpoint_save_best_only": true,
   "checkpoint_save_weights_only": false,
    "checkpoint_verbose": true,
    "tensorboard_write_graph": true
  }
}
